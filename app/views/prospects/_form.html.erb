<%= simple_form_for(@prospect) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_name %>
    <%= f.input :contact %>
    <%= f.input :contact_email %>
    <%= f.input :contact_title %>
    <%= f.input :company_description %>
    <%= f.input :company_url %>
    <%= f.input :company_image_url %>
    <%= f.input :company_bcorp_url %>
    <%= f.input :responded %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
